## DATA GENERATION
library(ggplot2)
set.seed(123)
n <- 20
x <- rbinom(n, 10, 0.5)
y <- rbinom(n, 10, 0.2)
z <- rbinom(n, 10, 0.8)
Nota <- c(x, y, z)
Asignatura <- c(rep('X', n), rep('Y', n), rep('Z', n))
data <- data.frame(Nota, Asignatura)
p <- ggplot(data, aes(x=Nota, y=Asignatura)) + geom_boxplot(width=0.5)
p
z <- c(177, 180, 193, 172, 173, 188, 185, 177, 168, 182)
mz <- sumz / n
mz
n <- length(x)
mz <- sumz / n
mz
z <- c(177, 180, 193, 172, 173, 188, 185, 177, 168, 182)
sumz <- sum(z)
mz <- sumz / n
mz
z
sum(z)/length(z)
mz
sumz
n
## DATA GENERATION
x <- c(175, 182, 190, 165, 172, 183, 187, 174, 168, 178)
n <- length(x)
mz <- sumz / n
s2z <- sumz2/n-mz^2
mz
s2z
sqrt(s2z)
