## DATA GENERATION
data <- data.reglin.opt.12
x <- data$x
y <- data$y
z <- data$z
n <- length(x)
## QUESTION/ANSWER GENERATION
# means
mx <- sum(x) / n
my <- sum(y) / n
mz <- sum(z) / n
# variances
s2x <- sum(x^2) / n - mx^2
s2y <- sum(y^2) / n - my^2
s2z <- sum(z^2) / n - mz^2
# Covariances
sxy <- sum(x*y) / n - mx*my
sxz <- sum(x*z) / n - mx*mz
# Detercmation coeff
r2xy <- sxy^2 / (s2x * s2y)
r2xz <- sxz^2 / (s2x * s2z)
# Regression lines
reglinexy <- lm(x~y)
reglinexz <- lm(x~z)
# Predicción
pred = reglinexy$coefficients[1] + reglinexy$coefficients[2]*60.
pred
